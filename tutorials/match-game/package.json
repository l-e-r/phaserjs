{
	"name": "match-game",
	"version": "1.0.0",
	"description": "A  Phaser 3 project using Parceljs",
	"scripts": {
		"start": "npx parcel src/index.html -p 8000",
		"build": "npx parcel build src/index.html & npm run copy-assets",
		"test": "echo \"Error: no test specified\" && exit 1",
		"copy-assets": "cp -r src/assets/* dist/*"
	},
	"author": "supertommy",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/ourcade/phaser3-parcel-template.git"
	},
	"homepage": "https://github.com/ourcade/phaser3-parcel-template",
	"devDependencies": {
		"@babel/core": "^7.10.5",
		"@babel/plugin-proposal-class-properties": "^7.10.4",
		"babel-eslint": "^10.1.0",
		"eslint": "^6.8.0",
		"minimist": ">=1.2.2",
		"parcel-plugin-clean-easy": "^1.0.2",
		"parcel-plugin-static-files-copy": "^2.4.3"
	},
	"dependencies": {
		"phaser": "^3.55.2"
	},
	"parcelCleanPaths": [
		"dist"
	],
	"staticFiles": {
		"staticPath": "src/assets",
		"watcherGlob": "**"
	}
}
